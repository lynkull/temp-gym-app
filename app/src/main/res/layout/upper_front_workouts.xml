<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#00FFFFFF">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="12dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Space
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="2" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="reps" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="weight" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="Inecline Dumbbell"
                android:textColor="#000000" />

            <Space
                android:layout_width="8dp"
                android:layout_height="wrap_content"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="8"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/inclineDumbbellRepsSet1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="3"
                    android:layout_weight="5"
                    android:hint="0"
                    android:inputType="number" />

                <EditText
                    android:id="@+id/inclineDumbbellRepsSet2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="3"
                    android:layout_weight="5"
                    android:hint="0"
                    android:inputType="number" />

                <EditText
                    android:id="@+id/inclineDumbbellRepsSet3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="8"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/inclineDumbbellWeightSet1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />

                <EditText
                    android:id="@+id/inclineDumbbellWeightSet2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />

                <EditText
                    android:id="@+id/inclineDumbbellWeightSet3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="Decline Dumbbell"
                android:textColor="#000000" />

            <Space
                android:layout_width="8dp"
                android:layout_height="wrap_content"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="8"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/declineDumbbellRepsSet1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="10"
                    android:hint="0"
                    android:inputType="number" />

                <EditText
                    android:id="@+id/declineDumbbellRepsSet2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="10"
                    android:hint="0"
                    android:inputType="number" />

                <EditText
                    android:id="@+id/declineDumbbellRepsSet3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="10"
                    android:hint="0"
                    android:inputType="number" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="8"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/declineDumbbellWeightSet1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="10"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />

                <EditText
                    android:id="@+id/declineDumbbellWeightSet2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="10"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />

                <EditText
                    android:id="@+id/declineDumbbellWeightSet3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="10"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="Lower Abbs"
                android:textColor="#000000" />

            <Space
                android:layout_width="8dp"
                android:layout_height="wrap_content"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="8"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/lowerAbbsRepsSet1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number" />

                <EditText
                    android:id="@+id/lowerAbbsRepsSet2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number" />

                <EditText
                    android:id="@+id/lowerAbbsRepsSet3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="8"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/lowerAbbsWeightSet1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />

                <EditText
                    android:id="@+id/lowerAbbsWeightSet2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />

                <EditText
                    android:id="@+id/lowerAbbsWeightSet3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="Tricep Pulldown"
                android:textColor="#000000" />

            <Space
                android:layout_width="8dp"
                android:layout_height="wrap_content"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="8"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/tricepPulldownRepsSet1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number" />

                <EditText
                    android:id="@+id/tricepPulldownRepsSet2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number" />

                <EditText
                    android:id="@+id/tricepPulldownRepsSet3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="8"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/tricepPulldownWeightSet1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />

                <EditText
                    android:id="@+id/tricepPulldownWeightSet2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />

                <EditText
                    android:id="@+id/tricepPulldownWeightSet3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="Bicep Curl"
                android:textColor="#000000" />

            <Space
                android:layout_width="8dp"
                android:layout_height="wrap_content"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="8"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/bicepCurlRepsSet1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number" />

                <EditText
                    android:id="@+id/bicepCurlRepsSet2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number" />

                <EditText
                    android:id="@+id/bicepCurlRepsSet3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="8"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/bicepCurlWeightSet1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />

                <EditText
                    android:id="@+id/bicepCurlWeightSet2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />

                <EditText
                    android:id="@+id/bicepCurlWeightSet3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="5"
                    android:ems="3"
                    android:hint="0"
                    android:inputType="number|numberDecimal" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>